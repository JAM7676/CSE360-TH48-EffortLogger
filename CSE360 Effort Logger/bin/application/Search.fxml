<AnchorPane prefHeight="535.0" prefWidth="470.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SearchController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="196.0" layoutY="29.0" prefHeight="27.0" prefWidth="83.0" text="SEARCH">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <TextField fx:id="searchField" layoutX="146.0" layoutY="118.0" prefHeight="27.0" prefWidth="181.0" />
      <Label layoutX="58.0" layoutY="124.0" text="Search for :">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="101.0" layoutY="63.0" text="Enter a keyword or project name to search!">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="104.0" layoutY="87.0" text="KEYWORDS: Effort Logs, Defect Logs, Projects">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <Button fx:id="button" layoutX="338.0" layoutY="119.0" mnemonicParsing="false" onAction="#searchButton" text="Enter" />
      <Label fx:id="resultLabel" alignment="CENTER" layoutX="161.0" layoutY="156.0" prefHeight="33.0" prefWidth="151.0" />
      <Button layoutX="184.0" layoutY="488.0" mnemonicParsing="false" onAction="#BackToMenu" text="Return To Menu" />
      <TextArea fx:id="outputBox" layoutX="45.0" layoutY="189.0" prefHeight="282.0" prefWidth="384.0" />
   </children>
</AnchorPane>
