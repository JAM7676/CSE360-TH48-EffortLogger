<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="286.0" prefWidth="354.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DefectLoggerController">
   <children>
      <ChoiceBox fx:id="projectSelect" layoutX="177.0" layoutY="80.0" prefWidth="150.0" />
      <Label layoutX="90.0" layoutY="23.0" text="Defect Logger">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="84.0" text="Select a Project:" />
      <Button fx:id="returnToMenuButton" layoutX="48.0" layoutY="232.0" mnemonicParsing="false" onAction="#BackToMainMenu" text="Return to Menu" />
      <Label fx:id="outputLabel" alignment="CENTER" layoutX="90.0" layoutY="264.0" prefHeight="17.0" prefWidth="174.0" />
      <TextArea fx:id="noteText" layoutX="49.0" layoutY="165.0" prefHeight="57.0" prefWidth="278.0" promptText="Enter Defect Log Here" />
      <ChoiceBox fx:id="defectSelect" layoutX="177.0" layoutY="126.0" prefWidth="150.0" />
      <Label layoutX="22.0" layoutY="130.0" text="Select a Defect Type:" />
      <Button fx:id="enterDefectButton" layoutX="202.0" layoutY="232.0" mnemonicParsing="false" onAction="#enterLog" text="Enter Defect Log" />
      <Label layoutX="14.0" layoutY="185.0" prefHeight="17.0" prefWidth="45.0" text="Note:" />
   </children>
</AnchorPane>
