         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <TextField fx:id="projectName" layoutX="183.0" layoutY="68.0" />
      <Label layoutX="27.0" layoutY="72.0" prefHeight="17.0" prefWidth="139.0" text="Enter New Project Name:" />
      <Button fx:id="newProjectButton" layoutX="119.0" layoutY="101.0" mnemonicParsing="false" onAction="#createNewProject" text="Create New Project" />
   </children>
</AnchorPane>
